<%= render partial: "my_navbar" %>
  <div class="new_container">
      <div class="main">
        <% @cocktails.each do |cocktail| %>
          
          <%= link_to cocktail_path(cocktail) do%>
            <div class="card" style="background-color:#282a36">
                  <div class="card-body">
                    <h5 class="card-title">
                    <%=cocktail.name%>
                    </h5> 
                    <%= cl_image_tag cocktail.photo.key, height: 300, width: 400, crop: :fill, class:"card-img-top" %>
                  </div>
                </div>
            <%end%>
        <%end%>
      </div>
  </div>
