<%= render partial: "my_navbar" %>
  <div class="new_container">
      <div class="main">
        <% @cocktails.each do |cocktail| %>
          
          <div class="card">
                <div class="card-body">
                  <h5 class="card-title">
                  <%=link_to cocktail.name, cocktail_path(cocktail), style:"text-decoration: none; color:#f8f8f2;" %></h5>
                  <%= cl_image_tag cocktail.photo.key, height: 300, width: 400, crop: :fill, class:"card-img-top" %>
                </div>
              </div>
        <%end%>
      </div>
  </div>
